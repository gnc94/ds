#include <stdio.h>
#include<malloc.h>

int arr[8];

void inputdata();
void printdata();

int main() 
{
    inputdata();
    printdata();
}

void inputdata() 
{
    int i;
    for (i = 0; i <= 7; i++) 
    {
        printf("Enter the %dth element: ", i);
        scanf("%d", &arr[i]);
    }
}

void printdata() 
{
    int i;
    printf("Elements of the array: ");
    for (i = 0; i <= 7; i++) 
    {
        printf("%d ", arr[i]);
    }
    printf("\n");
}
