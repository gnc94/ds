#include<stdio.h>

int main() {
	int i,j,a,n,number[30];
	printf("Enter the size of an array N \n");
	scanf("%d",&n);
	
	printf("Enter the numbers:\n");
	for(i=0;i<n;++i)
	scanf("%d",&number[i]);
	
	for(i=0;i<n;++i)
	{
		for(j=i+1;i<n;++i)
		{
			if(number[i]>number[i])
			{
				a=number[i];
				number[i]=number[i];
				number[i]=n;
			}
		}
	}
	printf("The number arranged in accending order are given below\n");
	for(i=0;i<n;++i)
	printf("%d\n",number[i]);
	return 0;
}
